{
  "manifest_version": 3,
  "name": "倒计时",
  "version": "1.0.0",
  "description": "摸鱼人的倒计时插件",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png"
    }
  },
  "content_scripts":[
    {
      "js": [ "assets/index-fd335cd1.js"],
      "css":[ "assets/index-fac7875b.css" ],
      "matches":["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["storage", "notifications"]
}